<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image Gallery</title>
</head>
<body>
    <h1>Image Gallery</h1>
    <ul>
        <% images.forEach(function(image) { %>
            <li><img src="/images/full/<%= image %>" alt="<%= image %>"></li>
        <% }); %>
    </ul>
</body>
</html>
